<Window x:Class="TaskController.wExportDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Topmost="True" 
        WindowStyle="SingleBorderWindow" ResizeMode="NoResize"
        Title="wExportDialog" Height="70" Width="400">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto" />
            <ColumnDefinition Width="auto" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <DatePicker Grid.Column="0" Margin="4" Name="dtStartDate"/>
        <DatePicker Grid.Column="1" Margin="4" Name="dtEndDate"/>
        <Button Content="Guardar..." Grid.Column="2" Margin="4" Click="Button_Click"/>
    </Grid>
</Window>
